[core]
        editor = vim
[alias]
        l = log --stat --color
        s = status -s
        ss = status
        a = add
        ps = push
        pl = pull
        c = commit
        ck = checkout
        ckm = checkout master
        cb = checkout -b
        d = diff --stat
        dd = diff
        ds = diff --staged
        m = merge
